{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      hoveredNextButton: false,\n      // Stato del pulsante \"Avanti\"\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\"\n      }\n    };\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    goToNextStep() {\n      this.loading = true; // Avvia il caricamento\n      setTimeout(() => {\n        this.currentStep++;\n        this.loading = false; // Ferma il caricamento\n      }, 2000); // Ritardo fittizio per il caricamento (ora 2 secondi)\n    },\n    goToPreviousStep() {\n      this.currentStep--;\n    },\n    isStepValid(step) {\n      if (step === 1) {\n        return this.form.nome && this.form.cognome && this.form.gender && this.form.data;\n      } else if (step === 2) {\n        return this.form.address && this.form.cap_code && this.form.tax_code && this.form.telefono;\n      } else if (step === 3) {\n        return this.form.email && this.form.password && this.form.confirmPassword;\n      }\n      return false;\n    }\n  }\n};","map":{"version":3,"names":["data","currentStep","loading","showPassword","showConfirmPassword","showPasswordError","hoveredNextButton","form","nome","cognome","gender","address","cap_code","tax_code","telefono","email","password","confirmPassword","methods","toggleShowPassword","toggleShowConfirmPassword","goToNextStep","setTimeout","goToPreviousStep","isStepValid","step"],"sources":["/Users/alessandromagnani/Desktop/Università/applicazioni-e-servizi-web/osteoarthritis-project/frontend/src/components/UserRegister.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <div class=\"container mt-5\">\n      <div class=\"header\">\n        <button\n          v-if=\"currentStep > 1\"\n          class=\"back-button\"\n          @click=\"goToPreviousStep\"\n        >\n          <img src=\"@/assets/back-icon.svg\" alt=\"Indietro\" class=\"back-icon\" />\n          <!-- Icona Indietro -->\n        </button>\n        <span class=\"step-title\">Passaggio {{ currentStep }} di 3</span>\n      </div>\n\n      <h2 class=\"mb-4\">Crea un Account</h2>\n\n      <form @submit.prevent=\"onSubmit\">\n        <div v-if=\"currentStep === 1\">\n          <!-- Nome -->\n          <div class=\"mb-3\">\n            <label for=\"nome\" class=\"form-label\">Nome</label>\n            <input\n              id=\"nome\"\n              v-model=\"form.nome\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n\n          <!-- Cognome -->\n          <div class=\"mb-3\">\n            <label for=\"cognome\" class=\"form-label\">Cognome</label>\n            <input\n              id=\"cognome\"\n              v-model=\"form.cognome\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n\n          <!-- Genere -->\n          <div class=\"mb-3\">\n            <label for=\"gender\" class=\"form-label\">Genere</label>\n            <select\n              id=\"gender\"\n              v-model=\"form.gender\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            >\n              <option value=\"Male\">Maschio</option>\n              <option value=\"Female\">Femmina</option>\n            </select>\n          </div>\n\n          <!-- Data di Nascita -->\n          <div class=\"mb-3\">\n            <label for=\"data\" class=\"form-label\">Data di Nascita</label>\n            <input\n              id=\"data\"\n              v-model=\"form.data\"\n              type=\"date\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n        </div>\n\n        <div v-if=\"currentStep === 2\">\n          <!-- Indirizzo -->\n          <div class=\"mb-3\">\n            <label for=\"address\" class=\"form-label\">Indirizzo</label>\n            <input\n              id=\"address\"\n              v-model=\"form.address\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n\n          <!-- CAP -->\n          <div class=\"mb-3\">\n            <label for=\"cap_code\" class=\"form-label\">CAP</label>\n            <input\n              id=\"cap_code\"\n              v-model=\"form.cap_code\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n\n          <!-- Codice Fiscale -->\n          <div class=\"mb-3\">\n            <label for=\"tax_code\" class=\"form-label\">Codice Fiscale</label>\n            <input\n              id=\"tax_code\"\n              v-model=\"form.tax_code\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n\n          <!-- Numero di Telefono -->\n          <div class=\"mb-3\">\n            <label for=\"telefono\" class=\"form-label\">Numero di Telefono</label>\n            <input\n              id=\"telefono\"\n              v-model=\"form.telefono\"\n              type=\"tel\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n        </div>\n\n        <div v-if=\"currentStep === 3\">\n          <!-- Email -->\n          <div class=\"mb-3\">\n            <label for=\"email\" class=\"form-label\">Email</label>\n            <input\n              id=\"email\"\n              v-model=\"form.email\"\n              type=\"email\"\n              class=\"form-control\"\n              required\n              style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n            />\n          </div>\n\n          <!-- Password -->\n          <div class=\"mb-3\">\n            <label for=\"password\" class=\"form-label\"\n              >Password\n              <span\n                class=\"info-icon\"\n                title=\"Requisiti per la password: min 8 caratteri, almeno una maiuscola, una minuscola e un numero.\"\n                >i</span\n              >\n            </label>\n            <div class=\"input-group\">\n              <input\n                id=\"password\"\n                v-model=\"form.password\"\n                :type=\"showPassword ? 'text' : 'password'\"\n                class=\"form-control password-input\"\n                required\n                @paste.prevent\n                style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n              />\n              <button\n                type=\"button\"\n                class=\"btn eye-button\"\n                @click=\"toggleShowPassword\"\n              >\n                <img\n                  :src=\"\n                    showPassword\n                      ? require('@/assets/eye-off.svg')\n                      : require('@/assets/eye-on.svg')\n                  \"\n                  alt=\"Toggle Password Visibility\"\n                  class=\"eye-icon\"\n                />\n              </button>\n            </div>\n            <div v-if=\"showPasswordError\" class=\"text-danger mt-1\">\n              Le password non corrispondono.\n            </div>\n          </div>\n\n          <!-- Conferma Password -->\n          <div class=\"mb-3\">\n            <label for=\"confirmPassword\" class=\"form-label\"\n              >Conferma Password</label\n            >\n            <div class=\"input-group\">\n              <input\n                id=\"confirmPassword\"\n                v-model=\"form.confirmPassword\"\n                :type=\"showConfirmPassword ? 'text' : 'password'\"\n                class=\"form-control password-input\"\n                :class=\"{ 'is-invalid': showPasswordError }\"\n                required\n                @paste.prevent\n                style=\"font-family: 'Universal Sans Text', Arial, sans-serif\"\n              />\n              <button\n                type=\"button\"\n                class=\"btn eye-button\"\n                @click=\"toggleShowConfirmPassword\"\n              >\n                <img\n                  :src=\"\n                    showConfirmPassword\n                      ? require('@/assets/eye-off.svg')\n                      : require('@/assets/eye-on.svg')\n                  \"\n                  alt=\"Toggle Password Visibility\"\n                  class=\"eye-icon\"\n                />\n              </button>\n            </div>\n          </div>\n        </div>\n\n        <!-- Pulsante Avanti -->\n        <button\n          type=\"button\"\n          class=\"btn btn-primary\"\n          :disabled=\"!isStepValid(currentStep)\"\n          @click=\"goToNextStep\"\n          @mouseover=\"hoveredNextButton = true\"\n          @mouseleave=\"hoveredNextButton = false\"\n          :style=\"hoveredNextButton ? 'cursor: not-allowed; opacity: 0.5;' : ''\"\n        >\n          Avanti\n        </button>\n      </form>\n    </div>\n\n    <!-- Loading Overlay -->\n    <div v-if=\"loading\" class=\"loading-overlay\">\n      <div class=\"loading-icon\">\n        <img src=\"@/assets/loading-icon.svg\" alt=\"Loading...\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      hoveredNextButton: false, // Stato del pulsante \"Avanti\"\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      },\n    };\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    goToNextStep() {\n      this.loading = true; // Avvia il caricamento\n      setTimeout(() => {\n        this.currentStep++;\n        this.loading = false; // Ferma il caricamento\n      }, 2000); // Ritardo fittizio per il caricamento (ora 2 secondi)\n    },\n    goToPreviousStep() {\n      this.currentStep--;\n    },\n    isStepValid(step) {\n      if (step === 1) {\n        return (\n          this.form.nome &&\n          this.form.cognome &&\n          this.form.gender &&\n          this.form.data\n        );\n      } else if (step === 2) {\n        return (\n          this.form.address &&\n          this.form.cap_code &&\n          this.form.tax_code &&\n          this.form.telefono\n        );\n      } else if (step === 3) {\n        return (\n          this.form.email && this.form.password && this.form.confirmPassword\n        );\n      }\n      return false;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  background: linear-gradient(135deg, #f7f7f7, #e0e0e0);\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff;\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n  margin-top: 100px; /* Sposta tutto un po' più in basso */\n}\n\n.header {\n  display: flex;\n  align-items: center;\n  justify-content: flex-start; /* Modificato per allineare a sinistra */\n  margin-bottom: 20px;\n}\n\n.back-button {\n  background: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.back-icon {\n  width: 24px; /* Dimensione dell'icona Indietro */\n  margin-right: 10px; /* Spazio tra icona e titolo */\n}\n\n.step-title {\n  font-size: 0.9rem; /* Rende il titolo più piccolo */\n  color: #495057;\n}\n\nh2 {\n  font-size: 1.5rem; /* Rende il titolo più piccolo */\n  color: #343a40;\n}\n\n.form-label {\n  font-size: 0.8rem; /* Rende le etichette più piccole */\n  color: #495057;\n}\n\n.invalid-feedback {\n  color: #dc3545;\n}\n\n.btn {\n  margin-top: 20px;\n  width: 100%; /* Rende il pulsante Avanti più largo */\n  background-color: #007bff; /* Colore del pulsante principale */\n  color: white; /* Colore del testo del pulsante */\n  font-size: 1rem; /* Rende il testo del pulsante un po' più piccolo */\n}\n\n.info-icon {\n  color: #6c757d; /* Colore grigio per l'icona informativa */\n  font-weight: bold;\n}\n\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(255, 255, 255, 0.8);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n.loading-icon {\n  width: 50px;\n}\n\n.eye-button {\n  background: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.eye-icon {\n  width: 20px;\n}\n</style>\n"],"mappings":"AAmPA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,OAAO,EAAE,KAAK;MACdC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,iBAAiB,EAAE,KAAK;