{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"RegisterComponent\",\n  data() {\n    return {\n      currentStep: 1,\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\"\n      },\n      errors: {},\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      showPasswordRequirements: false\n    };\n  },\n  computed: {\n    isStepValid() {\n      // Controlla se tutti i campi del passo corrente sono compilati\n      if (this.currentStep === 1) {\n        return this.form.nome && this.form.cognome && this.form.gender && this.form.data;\n      } else if (this.currentStep === 2) {\n        return this.form.address && this.form.cap_code && this.form.tax_code && this.form.telefono;\n      } else if (this.currentStep === 3) {\n        return this.form.email && this.form.password && this.form.confirmPassword;\n      }\n      return false;\n    }\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    nextStep() {\n      this.loading = true;\n      setTimeout(() => {\n        if (this.currentStep < 3) {\n          this.currentStep++;\n        }\n        this.loading = false;\n      }, 1000); // Tempo di caricamento prima di passare alla pagina successiva\n    },\n    prevStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    handleAfterLeave() {\n      if (this.loading) {\n        this.loading = false; // Nascondi la pagina corrente solo dopo il caricamento\n      }\n    },\n    async onSubmit() {\n      this.loading = true;\n      this.errors = {};\n      this.showPasswordError = false;\n\n      // Controllo delle password\n      if (this.form.password !== this.form.confirmPassword) {\n        this.showPasswordError = true;\n        this.loading = false;\n        return;\n      }\n      try {\n        const response = await axios.post(\"http://your-api-url/register\", this.form);\n\n        // Esempio di utilizzo della risposta (personalizzalo come necessario)\n        if (response.status === 200) {\n          // Esegui azioni con successo, ad esempio mostrare un messaggio\n          alert(\"Registrazione completata con successo!\");\n          // Reindirizza l'utente o resettare il modulo\n          this.resetForm();\n        }\n      } catch (error) {\n        this.errors.general = error.response.data.message || \"Errore durante la registrazione\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","currentStep","form","nome","cognome","gender","address","cap_code","tax_code","telefono","email","password","confirmPassword","errors","loading","showPassword","showConfirmPassword","showPasswordError","showPasswordRequirements","computed","isStepValid","methods","toggleShowPassword","toggleShowConfirmPassword","nextStep","setTimeout","prevStep","handleAfterLeave","onSubmit","response","post","status","alert","resetForm","error","general","message"],"sources":["/Users/alessandromagnani/Desktop/Università/applicazioni-e-servizi-web/osteoarthritis-project/frontend/src/components/UserRegister.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <div class=\"container mt-5\">\n      <div class=\"step-label\">\n        <span>Passaggio {{ currentStep }} di 3</span>\n      </div>\n      <h2 class=\"mb-4 text-center\">Crea il tuo Account</h2>\n\n      <form @submit.prevent=\"onSubmit\">\n        <transition name=\"fade\" @after-leave=\"handleAfterLeave\">\n          <div v-if=\"currentStep === 1\">\n            <div class=\"mb-3\">\n              <label for=\"nome\" class=\"form-label\">Nome</label>\n              <input\n                id=\"nome\"\n                v-model=\"form.nome\"\n                type=\"text\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"cognome\" class=\"form-label\">Cognome</label>\n              <input\n                id=\"cognome\"\n                v-model=\"form.cognome\"\n                type=\"text\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"gender\" class=\"form-label\">Genere</label>\n              <select\n                id=\"gender\"\n                v-model=\"form.gender\"\n                class=\"form-control\"\n                required\n              >\n                <option value=\"\" disabled>Seleziona Genere</option>\n                <option value=\"Maschio\">Maschio</option>\n                <option value=\"Femmina\">Femmina</option>\n                <option value=\"Altro\">Altro</option>\n              </select>\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"data\" class=\"form-label\">Data di Nascita</label>\n              <input\n                id=\"data\"\n                v-model=\"form.data\"\n                type=\"date\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n          </div>\n        </transition>\n\n        <transition name=\"fade\" @after-leave=\"handleAfterLeave\">\n          <div v-if=\"currentStep === 2\">\n            <div class=\"mb-3\">\n              <label for=\"address\" class=\"form-label\">Indirizzo</label>\n              <input\n                id=\"address\"\n                v-model=\"form.address\"\n                type=\"text\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"cap_code\" class=\"form-label\">Codice CAP</label>\n              <input\n                id=\"cap_code\"\n                v-model=\"form.cap_code\"\n                type=\"text\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"tax_code\" class=\"form-label\">Codice Fiscale</label>\n              <input\n                id=\"tax_code\"\n                v-model=\"form.tax_code\"\n                type=\"text\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"telefono\" class=\"form-label\"\n                >Numero di Telefono</label\n              >\n              <input\n                id=\"telefono\"\n                v-model=\"form.telefono\"\n                type=\"tel\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n          </div>\n        </transition>\n\n        <transition name=\"fade\" @after-leave=\"handleAfterLeave\">\n          <div v-if=\"currentStep === 3\">\n            <div class=\"mb-3\">\n              <label for=\"email\" class=\"form-label\">Email</label>\n              <input\n                id=\"email\"\n                v-model=\"form.email\"\n                type=\"email\"\n                class=\"form-control\"\n                required\n              />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"password\" class=\"form-label\">\n                Password\n                <span\n                  class=\"info-icon\"\n                  @mouseover=\"showPasswordRequirements = true\"\n                  @mouseleave=\"showPasswordRequirements = false\"\n                  >ⓘ</span\n                >\n              </label>\n              <div class=\"input-group\">\n                <input\n                  id=\"password\"\n                  v-model=\"form.password\"\n                  :type=\"showPassword ? 'text' : 'password'\"\n                  class=\"form-control password-input\"\n                  required\n                  @paste.prevent\n                />\n                <button\n                  type=\"button\"\n                  class=\"btn eye-button\"\n                  @click=\"toggleShowPassword\"\n                >\n                  <img\n                    :src=\"\n                      showPassword\n                        ? require('@/assets/eye-off.svg')\n                        : require('@/assets/eye-on.svg')\n                    \"\n                    alt=\"Toggle Password Visibility\"\n                    class=\"eye-icon\"\n                  />\n                </button>\n              </div>\n              <div v-if=\"showPasswordError\" class=\"text-danger mt-1\">\n                Le password non corrispondono.\n              </div>\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"confirmPassword\" class=\"form-label\"\n                >Conferma Password</label\n              >\n              <div class=\"input-group\">\n                <input\n                  id=\"confirmPassword\"\n                  v-model=\"form.confirmPassword\"\n                  :type=\"showConfirmPassword ? 'text' : 'password'\"\n                  class=\"form-control password-input\"\n                  required\n                />\n                <button\n                  type=\"button\"\n                  class=\"btn eye-button\"\n                  @click=\"toggleShowConfirmPassword\"\n                >\n                  <img\n                    :src=\"\n                      showConfirmPassword\n                        ? require('@/assets/eye-off.svg')\n                        : require('@/assets/eye-on.svg')\n                    \"\n                    alt=\"Toggle Password Visibility\"\n                    class=\"eye-icon\"\n                  />\n                </button>\n              </div>\n            </div>\n          </div>\n        </transition>\n\n        <div class=\"d-flex justify-content-between\">\n          <button\n            type=\"button\"\n            class=\"btn btn-secondary\"\n            @click=\"prevStep\"\n            :disabled=\"currentStep === 1\"\n          >\n            Precedente\n          </button>\n          <button\n            type=\"button\"\n            class=\"btn btn-primary\"\n            @click=\"nextStep\"\n            :disabled=\"!isStepValid\"\n            v-if=\"currentStep < 3\"\n          >\n            Avanti\n          </button>\n          <button\n            type=\"submit\"\n            class=\"btn btn-primary\"\n            v-if=\"currentStep === 3\"\n            :disabled=\"loading\"\n          >\n            <span v-if=\"loading\">Registrazione...</span>\n            <span v-else>Registrati</span>\n          </button>\n        </div>\n\n        <!-- Messaggio di errore -->\n        <div v-if=\"errors.general\" class=\"invalid-feedback mt-3\">\n          {{ errors.general }}\n        </div>\n      </form>\n\n      <div v-if=\"loading\" class=\"loading-overlay\">\n        <div class=\"loading-content\">\n          <div class=\"spinner-border text-light\" role=\"status\">\n            <span class=\"visually-hidden\">Caricamento...</span>\n          </div>\n          <p class=\"loading-text\">Caricamento in corso...</p>\n        </div>\n      </div>\n\n      <!-- Tooltip per la password -->\n      <div v-if=\"showPasswordRequirements\" class=\"tooltip\">\n        <p>Requisiti per la password:</p>\n        <ul>\n          <li>Minimo 8 caratteri</li>\n          <li>Almeno una lettera maiuscola</li>\n          <li>Almeno un numero</li>\n          <li>Almeno un simbolo</li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"RegisterComponent\",\n  data() {\n    return {\n      currentStep: 1,\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      },\n      errors: {},\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      showPasswordRequirements: false,\n    };\n  },\n  computed: {\n    isStepValid() {\n      // Controlla se tutti i campi del passo corrente sono compilati\n      if (this.currentStep === 1) {\n        return (\n          this.form.nome &&\n          this.form.cognome &&\n          this.form.gender &&\n          this.form.data\n        );\n      } else if (this.currentStep === 2) {\n        return (\n          this.form.address &&\n          this.form.cap_code &&\n          this.form.tax_code &&\n          this.form.telefono\n        );\n      } else if (this.currentStep === 3) {\n        return (\n          this.form.email && this.form.password && this.form.confirmPassword\n        );\n      }\n      return false;\n    },\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    nextStep() {\n      this.loading = true;\n      setTimeout(() => {\n        if (this.currentStep < 3) {\n          this.currentStep++;\n        }\n        this.loading = false;\n      }, 1000); // Tempo di caricamento prima di passare alla pagina successiva\n    },\n    prevStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    handleAfterLeave() {\n      if (this.loading) {\n        this.loading = false; // Nascondi la pagina corrente solo dopo il caricamento\n      }\n    },\n    async onSubmit() {\n      this.loading = true;\n      this.errors = {};\n      this.showPasswordError = false;\n\n      // Controllo delle password\n      if (this.form.password !== this.form.confirmPassword) {\n        this.showPasswordError = true;\n        this.loading = false;\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://your-api-url/register\",\n          this.form\n        );\n\n        // Esempio di utilizzo della risposta (personalizzalo come necessario)\n        if (response.status === 200) {\n          // Esegui azioni con successo, ad esempio mostrare un messaggio\n          alert(\"Registrazione completata con successo!\");\n          // Reindirizza l'utente o resettare il modulo\n          this.resetForm();\n        }\n      } catch (error) {\n        this.errors.general =\n          error.response.data.message || \"Errore durante la registrazione\";\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  max-width: 500px;\n  margin: auto;\n}\n\n.step-label {\n  font-size: 0.75rem; /* Dimensione piccola */\n  color: #6c757d; /* Colore grigio */\n  margin-bottom: 10px;\n}\n\nh2 {\n  font-size: 1.5rem; /* Dimensione del titolo */\n  color: #343a40;\n}\n\n.form-label {\n  color: #495057;\n  font-size: 0.875rem; /* Dimensione del testo */\n}\n\n.form-control {\n  border-radius: 0.375rem;\n  border: 1px solid #ced4da;\n  font-family: system, Arial, sans-serif;\n  font-size: 0.875rem; /* Dimensione del testo */\n}\n\n.btn {\n  border-radius: 0.375rem; /* Bordo arrotondato */\n  padding: 10px 20px; /* Padding per dimensione */\n  font-family: system, Arial, sans-serif;\n  font-size: 0.875rem; /* Dimensione del testo */\n}\n\n.btn-secondary {\n  background-color: rgba(240, 240, 240, 0.5); /* Colore più opaco */\n  color: #343a40;\n  border: none;\n}\n\n.btn-primary {\n  background-color: #007bff;\n  color: #fff;\n  border: none;\n}\n\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 9999;\n}\n\n.loading-content {\n  text-align: center;\n  color: #fff;\n}\n\n.loading-text {\n  margin-top: 10px;\n}\n\n.eye-button {\n  background: transparent;\n  border: none;\n}\n\n.eye-icon {\n  width: 20px; /* Dimensione dell'icona */\n}\n\n.info-icon {\n  color: #007bff;\n  cursor: pointer;\n  margin-left: 5px;\n  font-size: 0.9rem; /* Dimensione dell'icona informativa */\n}\n\n.tooltip {\n  position: absolute;\n  background-color: white;\n  border: 1px solid #ced4da;\n  border-radius: 4px;\n  padding: 10px;\n  z-index: 1000;\n  margin-top: 5px;\n  font-size: 0.8rem; /* Dimensione del testo tooltip */\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\n</style>\n"],"mappings":"AA+PA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVL,IAAI,EAAE,EAAE;QACRM,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE;MACnB,CAAC;MACDC,MAAM,EAAE,CAAC,CAAC;MACVC,OAAO,EAAE,KAAK;MACdC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,iBAAiB,EAAE,KAAK;MACxBC,wBAAwB,EAAE;IAC5B,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ;MACA,IAAI,IAAI,CAACnB,WAAU,KAAM,CAAC,EAAE;QAC1B,OACE,IAAI,CAACC,IAAI,CAACC,IAAG,IACb,IAAI,CAACD,IAAI,CAACE,OAAM,IAChB,IAAI,CAACF,IAAI,CAACG,MAAK,IACf,IAAI,CAACH,IAAI,CAACF,IAAG;MAEjB,OAAO,IAAI,IAAI,CAACC,WAAU,KAAM,CAAC,EAAE;QACjC,OACE,IAAI,CAACC,IAAI,CAACI,OAAM,IAChB,IAAI,CAACJ,IAAI,CAACK,QAAO,IACjB,IAAI,CAACL,IAAI,CAACM,QAAO,IACjB,IAAI,CAACN,IAAI,CAACO,QAAO;MAErB,OAAO,IAAI,IAAI,CAACR,WAAU,KAAM,CAAC,EAAE;QACjC,OACE,IAAI,CAACC,IAAI,CAACQ,KAAI,IAAK,IAAI,CAACR,IAAI,CAACS,QAAO,IAA