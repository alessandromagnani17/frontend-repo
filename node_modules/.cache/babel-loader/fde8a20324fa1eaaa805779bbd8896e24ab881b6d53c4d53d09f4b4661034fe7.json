gio di caricamento\n    });\n\n    EventBus.$on(\"loading-end\", () => {\n      this.loadingPrediction = false; // Nascondi il messaggio di caricamento\n    });\n\n    // Verifica il ruolo dell'utente loggato e stampa il messaggio in base al ruolo\n    const userRole = localStorage.getItem(\"userRole\");\n\n    if (userRole === \"doctor\") {\n      console.log(\"Sono un medico\");\n    } else if (userRole === \"patient\") {\n      console.log(\"Sono un paziente\");\n    } else {\n      console.log(\"Ruolo utente non identificato\");\n    }\n  },\n  beforeUnmount() {\n    // Rimuovi l'ascoltatore per evitare perdite di memoria\n    EventBus.$off(\"loading-start\");\n    EventBus.$off(\"loading-end\");\n  },\n};\n</script>\n\n<style scoped>\n/* Stili per il messaggio \"Nuovo caricamento\" */\n.loading-message {\n  font-size: 1.2em;\n  color: #ff4500; /* Colore per il messaggio di caricamento */\n  margin-top: 20px;\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAoB;;EADjCC,GAAA;EAKkCD,KAAK,EAAC;;;uBAJtCE,mBAAA,CASM,OATNC,UASM,G,0BARJC,mBAAA,CAA4B,YAAxB,qBAAmB,sBAEvBC,mBAAA,+EAAgF,EACrEC,KAAA,CAAAC,iBAAiB,I,cAA5BL,mBAAA,CAEM,OAFNM,UAEM,EAFgD,wBAEtD,KAPJH,mBAAA,gBASIA,mBAAA,qDAAwD,C","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}