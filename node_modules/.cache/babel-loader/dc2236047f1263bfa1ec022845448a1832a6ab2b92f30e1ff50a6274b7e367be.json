r);\n        alert(\"Error signing up: \" + error.message);\n\n        // Gestione degli errori\n        if (error.response && error.response.data) {\n          this.errors.general = error.response.data.error || \"Registration failed\";\n          alert(this.errors.general);\n        } else {\n          this.errors.general = error.message;\n          alert(\"Error: \" + error.message);\n        }\n      } finally {\n        this.loading = false;\n        console.log(\"Loading finished\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  background: linear-gradient(135deg, #f7f7f7, #e0e0e0);\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff;\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n}\n\nh2 {\n  font-size: 2.5rem;\n  color: #343a40;\n}\n\n.form-label {\n  color: #495057;\n}\n\n.invalid-feedback {\n  color: #dc3545;\n}\n\n.btn {\n  margin-top: 20px;\n  background-color: #007bff;\n  border: none;\n  transition: background-color 0.3s, transform 0.3s;\n}\n\n.btn:hover {\n  background-color: #0056b3;\n  transform: scale(1.05);\n}\n</style>\n"],"mappings":"AAwIA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QAAE;QACdC,IAAI,EAAE,EAAE;QAAE;QACVC,OAAO,EAAE,EAAE;QAAE;QACbJ,IAAI,EAAE,EAAE;QAAE;QACVK,SAAS,EAAE,EAAE;QAAE;QACfC,GAAG,EAAE,EAAE;QAAE;QACTC,aAAa,EAAE,EAAE;QAAE;QACnBC,QAAQ,EAAE,EAAE;QAAE;QACdC,QAAQ,EAAE,EAAE,CAAE;MAChB,CAAC;MACDC,MAAM,EAAE,CAAC,CAAC;MACVC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACfC,OAAO,CAACC,GAAG,CAAC,yBAAyB,CAAC;MACtC,IAAI,CAACJ,OAAM,GAAI,IAAI;MACnB,IAAI,CAACD,MAAK,GAAI,CAAC,CAAC;MAEhB,IAAI;QACF,MAAM;UAAER,QAAQ;UAAEO,QAAQ;UAAEN,IAAI;UAAEC,OAAO;UAAEJ,IAAI;UAAEK,SAAS;UAAEC,GAAG;UAAEC,aAAa;UAAEC;QAAS,IAAI,IAAI,CAACP,IAAI;QACtGa,OAAO,CAACC,GAAG,CAAC,YAAY,EAAE,IAAI,CAACd