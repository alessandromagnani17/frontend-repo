{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      form: {\n        nome: '',\n        cognome: '',\n        gender: '',\n        data: '',\n        address: '',\n        cap_code: '',\n        tax_code: '',\n        telefono: '',\n        email: '',\n        password: '',\n        confirmPassword: ''\n      }\n    };\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    goToNextStep() {\n      this.loading = true; // Avvia il caricamento\n      setTimeout(() => {\n        this.currentStep++;\n        this.loading = false; // Ferma il caricamento\n      }, 1000); // Ritardo fittizio per il caricamento\n    },\n    goToPreviousStep() {\n      this.currentStep--;\n    },\n    isStepValid(step) {\n      if (step === 1) {\n        return this.form.nome && this.form.cognome && this.form.gender && this.form.data;\n      } else if (step === 2) {\n        return this.form.address && this.form.cap_code && this.form.tax_code && this.form.telefono;\n      } else if (step === 3) {\n        return this.form.email && this.form.password && this.form.confirmPassword;\n      }\n      return false;\n    }\n  }\n};","map":{"version":3,"names":["data","currentStep","loading","showPassword","showConfirmPassword","showPasswordError","form","nome","cognome","gender","address","cap_code","tax_code","telefono","email","password","confirmPassword","methods","toggleShowPassword","toggleShowConfirmPassword","goToNextStep","setTimeout","goToPreviousStep","isStepValid","step"],"sources":["/Users/alessandromagnani/Desktop/Università/applicazioni-e-servizi-web/osteoarthritis-project/frontend/src/components/UserRegister.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <div class=\"container mt-5\">\n      <div class=\"header\">\n        <button v-if=\"currentStep > 1\" class=\"back-button\" @click=\"goToPreviousStep\">\n          &lt;\n        </button>\n        <span class=\"step-title\">Passaggio {{ currentStep }} di 3</span>\n      </div>\n      \n      <h2 class=\"mb-4\">Crea un Account</h2>\n      \n      <form @submit.prevent=\"onSubmit\">\n        <div v-if=\"currentStep === 1\">\n          <!-- Nome -->\n          <div class=\"mb-3\">\n            <label for=\"nome\" class=\"form-label\">Nome</label>\n            <input id=\"nome\" v-model=\"form.nome\" type=\"text\" class=\"form-control\" required />\n          </div>\n\n          <!-- Cognome -->\n          <div class=\"mb-3\">\n            <label for=\"cognome\" class=\"form-label\">Cognome</label>\n            <input id=\"cognome\" v-model=\"form.cognome\" type=\"text\" class=\"form-control\" required />\n          </div>\n\n          <!-- Genere -->\n          <div class=\"mb-3\">\n            <label for=\"gender\" class=\"form-label\">Genere</label>\n            <select id=\"gender\" v-model=\"form.gender\" class=\"form-control\" required>\n              <option value=\"Male\">Maschio</option>\n              <option value=\"Female\">Femmina</option>\n            </select>\n          </div>\n\n          <!-- Data di Nascita -->\n          <div class=\"mb-3\">\n            <label for=\"data\" class=\"form-label\">Data di Nascita</label>\n            <input id=\"data\" v-model=\"form.data\" type=\"date\" class=\"form-control\" required />\n          </div>\n        </div>\n\n        <div v-if=\"currentStep === 2\">\n          <!-- Indirizzo -->\n          <div class=\"mb-3\">\n            <label for=\"address\" class=\"form-label\">Indirizzo</label>\n            <input id=\"address\" v-model=\"form.address\" type=\"text\" class=\"form-control\" required />\n          </div>\n\n          <!-- CAP -->\n          <div class=\"mb-3\">\n            <label for=\"cap_code\" class=\"form-label\">CAP</label>\n            <input id=\"cap_code\" v-model=\"form.cap_code\" type=\"text\" class=\"form-control\" required />\n          </div>\n\n          <!-- Codice Fiscale -->\n          <div class=\"mb-3\">\n            <label for=\"tax_code\" class=\"form-label\">Codice Fiscale</label>\n            <input id=\"tax_code\" v-model=\"form.tax_code\" type=\"text\" class=\"form-control\" required />\n          </div>\n\n          <!-- Numero di Telefono -->\n          <div class=\"mb-3\">\n            <label for=\"telefono\" class=\"form-label\">Numero di Telefono</label>\n            <input id=\"telefono\" v-model=\"form.telefono\" type=\"tel\" class=\"form-control\" required />\n          </div>\n        </div>\n\n        <div v-if=\"currentStep === 3\">\n          <!-- Email -->\n          <div class=\"mb-3\">\n            <label for=\"email\" class=\"form-label\">Email</label>\n            <input id=\"email\" v-model=\"form.email\" type=\"email\" class=\"form-control\" required />\n          </div>\n\n          <!-- Password -->\n          <div class=\"mb-3\">\n            <label for=\"password\" class=\"form-label\">Password <span class=\"info-icon\" title=\"Requisiti per la password: min 8 caratteri, almeno una maiuscola, una minuscola e un numero.\">i</span></label>\n            <div class=\"input-group\">\n              <input id=\"password\" v-model=\"form.password\" :type=\"showPassword ? 'text' : 'password'\" class=\"form-control password-input\" required @paste.prevent />\n              <button type=\"button\" class=\"btn eye-button\" @click=\"toggleShowPassword\">\n                <img :src=\"showPassword ? require('@/assets/eye-off.svg') : require('@/assets/eye-on.svg')\" alt=\"Toggle Password Visibility\" class=\"eye-icon\" />\n              </button>\n            </div>\n            <div v-if=\"showPasswordError\" class=\"text-danger mt-1\">Le password non corrispondono.</div>\n          </div>\n\n          <!-- Conferma Password -->\n          <div class=\"mb-3\">\n            <label for=\"confirmPassword\" class=\"form-label\">Conferma Password</label>\n            <div class=\"input-group\">\n              <input id=\"confirmPassword\" v-model=\"form.confirmPassword\" :type=\"showConfirmPassword ? 'text' : 'password'\" class=\"form-control password-input\" :class=\"{ 'is-invalid': showPasswordError }\" required @paste.prevent />\n              <button type=\"button\" class=\"btn eye-button\" @click=\"toggleShowConfirmPassword\">\n                <img :src=\"showConfirmPassword ? require('@/assets/eye-off.svg') : require('@/assets/eye-on.svg')\" alt=\"Toggle Password Visibility\" class=\"eye-icon\" />\n              </button>\n            </div>\n          </div>\n        </div>\n\n        <!-- Pulsante Avanti -->\n        <button type=\"button\" class=\"btn btn-primary\" :disabled=\"!isStepValid(currentStep)\" @click=\"goToNextStep\">\n          Avanti\n        </button>\n      </form>\n    </div>\n\n    <!-- Loading Overlay -->\n    <div v-if=\"loading\" class=\"loading-overlay\">\n      <div class=\"loading-icon\">\n        <img src=\"loading-icon.svg\" alt=\"Loading...\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      form: {\n        nome: '',\n        cognome: '',\n        gender: '',\n        data: '',\n        address: '',\n        cap_code: '',\n        tax_code: '',\n        telefono: '',\n        email: '',\n        password: '',\n        confirmPassword: '',\n      },\n    };\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    goToNextStep() {\n      this.loading = true; // Avvia il caricamento\n      setTimeout(() => {\n        this.currentStep++;\n        this.loading = false; // Ferma il caricamento\n      }, 1000); // Ritardo fittizio per il caricamento\n    },\n    goToPreviousStep() {\n      this.currentStep--;\n    },\n    isStepValid(step) {\n      if (step === 1) {\n        return this.form.nome && this.form.cognome && this.form.gender && this.form.data;\n      } else if (step === 2) {\n        return this.form.address && this.form.cap_code && this.form.tax_code && this.form.telefono;\n      } else if (step === 3) {\n        return this.form.email && this.form.password && this.form.confirmPassword;\n      }\n      return false;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  background: linear-gradient(135deg, #f7f7f7, #e0e0e0);\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff;\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n  margin-top: 100px; /* Sposta tutto un po' più in basso */\n}\n\n.header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 20px;\n}\n\n.back-button {\n  background: transparent;\n  border: none;\n  color: lightgray;\n  font-size: 1.5rem;\n  cursor: pointer;\n}\n\n.step-title {\n  font-size: 0.9rem; /* Rende il titolo più piccolo */\n  color: #495057;\n}\n\nh2 {\n  font-size: 1.5rem; /* Rende il titolo più piccolo */\n  color: #343a40;\n}\n\n.form-label {\n  font-size: 0.9rem; /* Rende le etichette più piccole */\n  color: #495057;\n}\n\n.invalid-feedback {\n  color: #dc3545;\n}\n\n.btn {\n  margin-top: 20px;\n  width: 100%; /* Rende il pulsante Avanti più largo */\n  background-color: #007bff; /* Colore del pulsante principale */\n  color: white; /* Colore del testo del pulsante */\n  font-size: 1rem; /* Rende il testo del pulsante più piccolo */\n}\n\n.btn:disabled {\n  background-color: rgba(0, 123, 255, 0.5); /* Colore più opaco per pulsanti disabilitati */\n}\n\n.info-icon {\n  color: gray; /* Colore grigio per l'icona informativa */\n  font-weight: bold;\n}\n\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(255, 255, 255, 0.8);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n.loading-icon {\n  width: 50px;\n}\n\n.eye-button {\n  background: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.eye-icon {\n  width: 20px;\n}\n</style>\n"],"mappings":"AAoHA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,OAAO,EAAE,KAAK;MACdC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,iBAAiB,EAAE,KAAK;MACxBC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVT,IAAI,EAAE,EAAE;QACRU,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE;MACnB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACf,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IACDgB,yBAAyBA,CAAA,EAAG;MAC1B,IAAI,CAACf,mBAAkB,GAAI,CAAC,IAAI,CAACA,mBAAmB;IACtD,CAAC;IACDgB,YAAYA,CAAA,EAAG;MACb,IAAI,CAAClB,OAAM,GAAI,IAAI,EAAE;MACrBmB,UAAU,CAAC,MAAM;QACf,IAAI,CAACpB,WAAW,EAAE;QAClB,IAAI,CAACC,OAAM,GAAI,KAAK,EAAE;MACxB,CAAC,EAAE,IAAI,CAAC,EAAE;IACZ,CAAC;IACDoB,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACrB,WAAW,EAAE;IACpB,CAAC;IACDsB,WAAWA,CAACC,IAAI,EAAE;MAChB,IAAIA,IAAG,KAAM,CAAC,EAAE;QACd,OAAO,IAAI,CAAClB,IAAI,CAACC,IAAG,IAAK,IAAI,CAACD,IAAI,CAACE,OAAM,IAAK,IAAI,CAACF,IAAI,CAACG,MAAK,IAAK,IAAI,CAACH,IAAI,CAACN,IAAI;MAClF,OAAO,IAAIwB,IAAG,KAAM,CAAC,EAAE;QACrB,OAAO,IAAI,CAAClB,IAAI,CAACI,OAAM,IAAK,IAAI,CAACJ,IAAI,CAACK,QAAO,IAAK,IAAI,CAACL,IAAI,CAACM,QAAO,IAAK,IAAI,CAACN,IAAI,CAACO,QAAQ;MAC5F,OAAO,IAAIW,IAAG,KAAM,CAAC,EAAE;QACrB,OAAO,IAAI,CAAClB,IAAI,CAACQ,KAAI,IAAK,IAAI,CAACR,IAAI,CAACS,QAAO,IAAK,IAAI,CAACT,IAAI,CAACU,eAAe;MAC3E;MACA,OAAO,KAAK;IACd;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module",