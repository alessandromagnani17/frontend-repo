his.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    async registerUser() {\n      this.loading = true;\n      try {\n        const response = await fetch(\"http://localhost:5000/register\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n          body: JSON.stringify({\n            nome: this.form.nome,\n            cognome: this.form.cognome,\n            gender: this.form.gender,\n            data: this.form.data,\n            address: this.form.address,\n            cap_code: this.form.cap_code,\n            tax_code: this.form.tax_code,\n            telefono: this.form.telefono,\n            email: this.form.email,\n            password: this.form.password,\n          }),\n        });\n        if (response.ok) {\n          this.$router.push({ name: \"WelcomePage\" });\n        } else {\n          console.error(\"Registrazione fallita.\");\n        }\n      } catch (error) {\n        console.error(\"Errore:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Stili aggiornati per validazione e layout */\n.form-control.is-invalid {\n  border-color: red;\n}\n.text-danger ul {\n  margin: 0;\n  padding-left: 20px;\n}\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(255, 255, 255, 0.8);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.loading-icon {\n  width: 50px;\n  height: 50px;\n}\n</style>\n"],"mappings":"AAwOA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,OAAO,EAAE,KAAK;MACdC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,iBAAiB,EAAE,KAAK;MACxBC,cAAc,EAAE,EAAE;MAClBC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVV,IAAI,EAAE,EAAE;QACRW,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,E