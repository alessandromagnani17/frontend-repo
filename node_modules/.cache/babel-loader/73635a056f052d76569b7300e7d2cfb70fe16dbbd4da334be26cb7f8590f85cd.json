{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      passwordErrors: [],\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\"\n      }\n    };\n  },\n  methods: {\n    goToNextStep() {\n      if (this.currentStep === 3 && this.isStepValid(this.currentStep)) {\n        this.registerUser();\n      } else if (this.isStepValid(this.currentStep)) {\n        this.loading = true;\n        setTimeout(() => {\n          this.currentStep++;\n          this.loading = false;\n        }, 1000);\n      }\n    },\n    goToPreviousStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    isStepValid(step) {\n      switch (step) {\n        case 1:\n          return this.form.nome && this.form.cognome && this.form.gender && this.form.data;\n        case 2:\n          return this.form.address && this.form.cap_code && this.form.tax_code && this.form.telefono;\n        case 3:\n          const passwordsMatch = this.form.password === this.form.confirmPassword;\n          this.showPasswordError = !passwordsMatch;\n          return this.form.email && this.passwordErrors.length === 0 && passwordsMatch;\n        default:\n          return false;\n      }\n    },\n    validatePassword() {\n      const password = this.form.password;\n      this.passwordErrors = [];\n      if (!/[A-Z]/.test(password)) this.passwordErrors.push(\"Deve contenere almeno una lettera maiuscola.\");\n      if (!/[a-z]/.test(password)) this.passwordErrors.push(\"Deve contenere almeno una lettera minuscola.\");\n      if (!/\\d/.test(password)) this.passwordErrors.push(\"Deve contenere almeno un numero.\");\n      if (!/[!@#$%^&*(),.?\":{}|<>]/.test(password)) this.passwordErrors.push(\"Deve contenere almeno un carattere speciale.\");\n      if (password.length < 8) this.passwordErrors.push(\"La password deve essere di almeno 8 caratteri.\");\n    },\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    async registerUser() {\n      this.loading = true;\n      try {\n        const response = await fetch(\"http://localhost:5000/register\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\"\n          },\n          body: JSON.stringify({\n            nome: this.form.nome,\n            cognome: this.form.cognome,\n            gender: this.form.gender,\n            data: this.form.data,\n            address: this.form.address,\n            cap_code: this.form.cap_code,\n            tax_code: this.form.tax_code,\n            telefono: this.form.telefono,\n            email: this.form.email,\n            password: this.form.password\n          })\n        });\n        if (response.ok) {\n          this.$router.push({\n            name: \"WelcomePage\"\n          });\n        } else {\n          console.error(\"Registrazione fallita.\");\n        }\n      } catch (error) {\n        console.error(\"Errore:\", error);\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","currentStep","loading","showPassword","showConfirmPassword","showPasswordError","passwordErrors","form","nome","cognome","gender","address","cap_code","tax_code","telefono","email","password","confirmPassword","methods","goToNextStep","isStepValid","registerUser","setTimeout","goToPreviousStep","step","passwordsMatch","length","validatePassword","test","push","toggleShowPassword","toggleShowConfirmPassword","response","fetch","method","headers","body","JSON","st