oading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","currentStep","form","nome","cognome","gender","address","cap_code","tax_code","telefono","email","password","confirmPassword","errors","loading","showPassword","showConfirmPassword","showPasswordError","methods","toggleShowPassword","toggleShowConfirmPassword","nextStep","setTimeout","prevStep","onSubmit","response","post","alert","console","log","error","general","message"],"sources":["/Users/alessandromagnani/Desktop/UniversitaÌ€/applicazioni-e-servizi-web/osteoarthritis-project/frontend/src/components/UserRegister.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <div class=\"container mt-5\">\n      <h2 class=\"mb-4 text-center\">Crea il tuo Account</h2>\n      <div class=\"step-label\">\n        <span>Passaggio {{ currentStep }}</span>\n      </div>\n\n      <form @submit.prevent=\"onSubmit\">\n        <transition name=\"fade\">\n          <div v-if=\"currentStep === 1\">\n            <div class=\"mb-3\">\n              <label for=\"nome\" class=\"form-label\">Nome</label>\n              <input id=\"nome\" v-model=\"form.nome\" type=\"text\" class=\"form-control\" required />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"cognome\" class=\"form-label\">Cognome</label>\n              <input id=\"cognome\" v-model=\"form.cognome\" type=\"text\" class=\"form-control\" required />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"gender\" class=\"form-label\">Genere</label>\n              <select id=\"gender\" v-model=\"form.gender\" class=\"form-control\" required>\n                <option value=\"\" disabled>Seleziona Genere</option>\n                <option value=\"Maschio\">Maschio</option>\n                <option value=\"Femmina\">Femmina</option>\n                <option value=\"Altro\">Altro</option>\n              </select>\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"data\" class=\"form-label\">Data di Nascita</label>\n              <input id=\"data\" v-model=\"form.data\" type=\"date\" class=\"form-control\" required />\n            </div>\n          </div>\n        </transition>\n\n        <transition name=\"fade\">\n          <div v-if=\"currentStep === 2\">\n            <div class=\"mb-3\">\n              <label for=\"address\" class=\"form-label\">Indirizzo</label>\n              <input id=\"address\" v-model=\"form.address\" type=\"text\" class=\"form-control\" required />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"cap_code\" class=\"form-label\">Codice CAP</label>\n              <input id=\"cap_code\" v-model=\"form.cap_code\" type=\"text\" class=\"form-control\" required />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"tax_code\" class=\"form-label\">Codice Fiscale</label>\n              <input id=\"tax_code\" v-model=\"form.tax_code\" type=\"text\" class=\"form-control\" required />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"telefono\" class=\"form-label\">Numero di Telefono</label>\n              <input id=\"telefono\" v-model=\"form.telefono\" type=\"tel\" class=\"form-control\" required />\n            </div>\n          </div>\n        </transition>\n\n        <transition name=\"fade\">\n          <div v-if=\"currentStep === 3\">\n            <div class=\"mb-3\">\n              <label for=\"email\" class=\"form-label\">Email</label>\n              <input id=\"email\" v-model=\"form.email\" type=\"email\" class=\"form-control\" required />\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"password\" class=\"form-label\">Password</label>\n              <div class=\"input-group\">\n                <input\n                  id=\"password\"\n                  v-model=\"form.password\"\n                  :type=\"showPassword ? 'text' : 'password'\"\n                  class=\"form-control password-input\"\n                  required\n                  @paste.prevent\n                />\n                <button type=\"button\" class=\"btn eye-button\" @click=\"toggleShowPassword\">\n                  <img :src=\"showPassword ? require('@/assets/eye-off.svg') : require('@/assets/eye-on.svg')\" alt=\"Toggle Password Visibility\" class=\"eye-icon\" />\n                </button>\n              </div>\n              <div v-if=\"showPasswordError\" class=\"text-danger mt-1\">Le password non corrispondono.</div>\n            </div>\n\n            <div class=\"mb-3\">\n              <label for=\"confirmPassword\" class=\"form-label\">Conferma Password</label>\n              <div class=\"input-group\">\n                <input\n                  id=\"confirmPassword\"\n                  v-model=\"form.confirmPassword\"\n                  :type=\"showConfirmPassword ? 'text' : 'password'\"\n                  class=\"form-control password-input\"\n                  required\n                />\n                <button type=\"button\" class=\"btn eye-button\" @click=\"toggleShowConfirmPassword\">\n                  <img :src=\"showConfirmPassword ? require('@/assets/eye-off.svg') : require('@/assets/eye-on.svg')\" alt=\"Toggle Password Visibility\" class=\"eye-icon\" />\n                </button>\n              </div>\n            </div>\n          </div>\n        </transition>\n\n        <div class=\"d-flex justify-content-between\">\n          <button type=\"button\" class=\"btn btn-secondary\" @click=\"prevStep\" :disabled=\"currentStep === 1\">Precedente</button>\n          <button type=\"button\" class=\"btn btn-primary\" @click=\"nextStep\" v-if=\"currentStep < 3\">Avanti</button>\n          <button type=\"submit\" class=\"btn btn-primary\" v-if=\"currentStep === 3\" :disabled=\"loading\">\n            <span v-if=\"loading\">Registrazione...</span>\n            <span v-else>Registrati</span>\n          </button>\n        </div>\n\n        <!-- Messaggio di errore -->\n        <div v-if=\"errors.general\" class=\"invalid-feedback mt-3\">{{ errors.general }}</div>\n      </form>\n\n      <!-- Caricamento stilizzato -->\n      <div v-if=\"loading\" class=\"loading-overlay\">\n        <div class=\"spinner-border text-light\" role=\"status\">\n          <span class=\"visually-hidden\">Caricamento...</span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"RegisterComponent\",\n  data() {\n    return {\n      currentStep: 1,\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      },\n      errors: {},\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n    };\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    nextStep() {\n      if (this.currentStep < 3) {\n        this.currentStep++;\n        this.loading = true;\n        setTimeout(() => {\n          this.loading = false;\n        }, 500); // Simula il caricamento\n      }\n    },\n    prevStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    async onSubmit() {\n      this.loading = true;\n      this.errors = {};\n      this.showPasswordError = false;\n\n      // Controllo delle password\n      if (this.form.password !== this.form.confirmPassword) {\n        this.showPasswordError = true;\n        this.loading = false;\n        return;\n      }\n\n      try {\n        const response = await axios.post(\"http://127.0.0.1:5000/register\", this.form);\n        alert(\"Registrazione avvenuta con successo! Controlla la tua email per la verifica.\");\n        console.log(response.data);\n      } catch (error) {\n        console.error(\"Errore durante la registrazione:\", error);\n        if (error.response) {\n          this.errors.general = error.response.data.error || \"Registrazione fallita\";\n        } else {\n          this.errors.general = error.message;\n        }\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  background: linear-gradient(135deg, #f7f7f7, #e0e0e0);\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff;\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n}\n\n.step-label {\n  font-size: 0.9rem;\n  color: #b0b0b0;\n  margin-bottom: 15px;\n}\n\nh2 {\n  font-size: 2rem; /* Dimensione del titolo */\n  color: #343a40;\n  font-family: 'Helvetica Neue', sans-serif;\n}\n\n.form-label {\n  color: #495057;\n}\n\n.invalid-feedback {\n  color: #dc3545;\n}\n\n.btn-primary {\n  background-color: #007bff;\n  border-color: #007bff;\n  font-family: 'Helvetica Neue', sans-serif;\n}\n\n.btn-primary:hover {\n  background-color: #0056b3;\n  border-color: #0056b3;\n}\n\n.eye-button {\n  background: none;\n  border: none;\n}\n\n.eye-icon {\n  width: 20px;\n  height: 20px;\n}\n\n.password-input {\n  flex: 1;\n}\n\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n.spinner-border {\n  width: 3rem;\n  height: 3rem;\n}\n\n.fade-enter-active, .fade-leave-active {\n  transition: opacity 0.5s;\n}\n.fade-enter, .fade-leave-to {\n  opacity: 0;\n}\n</style>\n"],"mappings":"AAiIA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IA