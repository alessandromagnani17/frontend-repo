{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      radiographs: [],\n      role: \"\"\n    };\n  },\n  created() {\n    const userDataString = localStorage.getItem(\"userData\");\n    if (userDataString) {\n      const userData = JSON.parse(userDataString);\n      const userUID = userData.uid;\n      this.role = userData.role;\n      console.log(\"UID dell'utente:\", userUID);\n      console.log(\"Role dell'utente:\", this.role);\n      this.fetchRadiographs(userUID);\n    } else {\n      console.log(\"Nessun dato utente trovato in localStorage.\");\n    }\n  },\n  methods: {\n    async fetchRadiographs(userUID) {\n      try {\n        const response = await axios.get(`/api/radiographs/${userUID}`);\n        this.radiographs = response.data;\n        console.log(\"Radiografie recuperate:\", this.radiographs);\n      } catch (error) {\n        console.error(\"Errore nel recupero delle radiografie:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","radiographs","role","created","userDataString","localStorage","getItem","userData","JSON","parse","userUID","uid","console","log","fetchRadiographs","methods","response","get","error"],"sources":["C:\\Users\\Utente\\Desktop\\osteoarthritis-project\\frontend\\src\\views\\ViewRadiographs.vue"],"sourcesContent":["<template>\n  <div class=\"welcome\">\n    <div class=\"container mt-5\">\n      <h5><strong>Visualizza una radiografia</strong></h5>\n      <div v-if=\"role === 'doctor'\">\n        <p>You have access to all patients' radiographs.</p>\n        <div class=\"cards-container\">\n          <div\n            v-for=\"radiograph in radiographs\"\n            :key=\"radiograph.id\"\n            class=\"radiograph-card\"\n          >\n            <img :src=\"radiograph.url\" alt=\"Radiograph\" />\n            <p>{{ radiograph.description }}</p>\n          </div>\n        </div>\n      </div>\n      <div v-else-if=\"role === 'patient'\">\n        <p>You can only see your own radiographs.</p>\n      </div>\n      <div v-else>\n        <p>You do not have permission to view radiographs.</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      radiographs: [],\n      role: \"\",\n    };\n  },\n  created() {\n    const userDataString = localStorage.getItem(\"userData\");\n    if (userDataString) {\n      const userData = JSON.parse(userDataString);\n      const userUID = userData.uid;\n      this.role = userData.role;\n      console.log(\"UID dell'utente:\", userUID);\n      console.log(\"Role dell'utente:\", this.role);\n\n      this.fetchRadiographs(userUID);\n    } else {\n      console.log(\"Nessun dato utente trovato in localStorage.\");\n    }\n  },\n  methods: {\n    async fetchRadiographs(userUID) {\n      try {\n        const response = await axios.get(`/api/radiographs/${userUID}`);\n        this.radiographs = response.data;\n        console.log(\"Radiografie recuperate:\", this.radiographs);\n      } catch (error) {\n        console.error(\"Errore nel recupero delle radiografie:\", error);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.view-radiographs {\n  background: #ffffff;\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  box-shadow: 0 0px 0px rgba(0, 0, 0, 0);\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff;\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n  margin-top: 100px;\n  height: auto;\n  text-align: center;\n}\n\nh2 {\n  font-size: 18px;\n  margin-bottom: 30px;\n}\n\n.cards-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n  justify-content: center;\n}\n\n.radiograph-card {\n  width: 300px;\n  padding: 10px;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n