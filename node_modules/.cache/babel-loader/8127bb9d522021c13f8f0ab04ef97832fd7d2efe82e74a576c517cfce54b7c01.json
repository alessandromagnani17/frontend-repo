{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"RegisterComponent\",\n  data() {\n    return {\n      currentStep: 1,\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\"\n      },\n      errors: {},\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false\n    };\n  },\n  methods: {\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    nextStep() {\n      this.loading = true;\n      setTimeout(() => {\n        if (this.currentStep < 3) {\n          this.currentStep++;\n        }\n        this.loading = false;\n      }, 1000); // Tempo di caricamento prima di passare alla pagina successiva\n    },\n    prevStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    handleAfterLeave() {\n      if (this.loading) {\n        this.loading = false; // Nascondi la pagina corrente solo dopo il caricamento\n      }\n    },\n    async onSubmit() {\n      this.loading = true;\n      this.errors = {};\n      this.showPasswordError = false;\n\n      // Controllo delle password\n      if (this.form.password !== this.form.confirmPassword) {\n        this.showPasswordError = true;\n        this.loading = false;\n        return;\n      }\n      try {\n        const response = await axios.post(\"http://127.0.0.1:5000/register\", this.form);\n        alert(\"Registrazione avvenuta con successo! Controlla la tua email per la verifica.\");\n        console.log(response.data);\n      } catch (error) {\n        console.error(\"Errore durante la registrazione:\", error);\n        if (error.response) {\n    