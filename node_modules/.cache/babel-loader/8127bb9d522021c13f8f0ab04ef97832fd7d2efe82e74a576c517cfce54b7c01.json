ubmit() {\n      this.loading = true;\n      this.errors = {};\n      this.showPasswordError = false;\n\n      // Controllo delle password\n      if (this.form.password !== this.form.confirmPassword) {\n        this.showPasswordError = true;\n        this.loading = false;\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://127.0.0.1:5000/register\",\n          this.form\n        );\n        alert(\n          \"Registrazione avvenuta con successo! Controlla la tua email per la verifica.\"\n        );\n        console.log(response.data);\n      } catch (error) {\n        console.error(\"Errore durante la registrazione:\", error);\n        if (error.response) {\n          this.errors.general =\n            error.response.data.error || \"Registrazione fallita\";\n        } else {\n          this.errors.general = error.message;\n        }\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  background: linear-gradient(135deg, #f7f7f7, #e0e0e0);\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff;\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n}\n\n.step-label {\n  font-size: 1rem;\n  color: #b0b0b0;\n  margin-bottom: 15px;\n  font-family: \"Helvetica Neue\", sans-serif; /* Cambia font se necessario */\n}\n\nh2 {\n  font-size: 2rem; /* Dimensione del titolo */\n  color: #343a40;\n  font-family: \"Helvetica Neue\", sans-serif;\n}\n\n.form-label {\n  color: #495057;\n  font-family: \"Helvetica Neue\", sans-serif;\n}\n\n.form-control {\n  border-radius: 0.375rem;\n  border: 1px solid #ced4da;\n}\n\n.btn {\n  border-radius: 0.375rem; /* Bordo arrotondato */\n  padding: 10px 20px; /* Padding per dimensione */\n  font-family: \"Helvetica Neue\", sans-serif;\n}\n\n.btn-secondary {\n  background-color: #f0f0f0;\n  color: #343a40;\n  border: none;\n}\n\n.btn-primary {\n  background-color: #007bff;\n  color: #fff;\n  border: none;\n}\n\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 9999;\n}\n\n.loading-content {\n  text-align: center;\n  color: #fff;\n}\n\n.loading-text {\n  margin-top: 10px;\n}\n\n.eye-button {\n  background: transparent;\n  border: none;\n}\n\n.eye-icon {\n  width: 20px; /* Dimensione dell'icona */\n}\n</style>\n"],"mappings":"AA4OA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVL,IAAI,EAAE,EAAE;QACRM,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE;MACnB,CAAC;MACDC,MAAM,EAAE,CAAC,CAAC;MACVC,OAAO,EAAE,KAAK;MACdC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACJ,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IACDK,yBAAyBA,CAAA,EAAG;MAC1B,IAAI,CAACJ,mBAAkB,GAAI,CAAC,IAAI,CAACA,mBAAmB;IACtD,CAAC;IACDK,QAAQA,CAAA,EAAG;MACT,IAAI,CAACP,OAAM,GAAI,IAAI;MACnBQ,UAAU,CAAC,MAAM;QACf,IAAI,IAAI,CAACrB,WAAU,GAAI,CAAC,EAAE;UACxB,IAAI,CAACA,WAAW,EAAE;QACpB;QACA,IAAI,CAACa,OAAM,GAAI,KAAK;MACtB,CAAC,EAAE,IAAI,CAAC,EAAE;IACZ,CAAC;IACDS,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACtB,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF,CAAC;IACDuB,gBAAgBA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACV,OAAO,EAAE;QAChB,IAAI,CAACA,OAAM,GAAI,KAAK,EAAE;MACxB;IACF,CAAC;IACD,MAAMW,QAAQA,CAAA,EAAG;MACf,IAAI,CAACX,OAAM,GAAI,IAAI;MACnB,IAAI,CAACD,MAAK,GAAI,CAAC,CAAC;MAChB,IAAI,CAACI,iBAAgB,GAAI,KAAK;;MAE9B;MACA,IAAI,IAAI,CAACf,IAAI,CAACS,QAAO,KAAM,IAAI,CAACT,IAAI,CAACU,eAAe,EAAE;QACpD,IAAI,CAACK,iBAAgB,GAAI,IAAI;QAC7B,IAAI,CAACH,OAAM,GAAI,KA