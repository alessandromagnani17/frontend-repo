{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\"\n      }\n    };\n  },\n  methods: {\n    goToNextStep() {\n      if (this.isStepValid(this.currentStep)) {\n        this.currentStep++;\n      }\n    },\n    goToPreviousStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    isStepValid(step) {\n      switch (step) {\n        case 1:\n          {\n            return this.form.nome && this.form.cognome && this.form.gender && this.form.data;\n          }\n        case 2:\n          {\n            return this.form.address && this.form.cap_code && this.form.tax_code && this.form.telefono;\n          }\n        case 3:\n          {\n            const passwordsMatch = this.form.password === this.form.confirmPassword;\n            this.showPasswordError = !passwordsMatch;\n            return this.form.email && this.form.password && passwordsMatch;\n          }\n        default:\n          return false;\n      }\n    },\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    onSubmit() {\n      this.loading = true; // Mostra il caricamento\n      console.log(this.form);\n      // Simula un caricamento\n      setTimeout(() => {\n        this.loading = false; // Nascondi il caricamento dopo 2 secondi\n      }, 2000);\n    }\n  }\n};","map":{"version":3,"names":["data","currentStep","loading","showPassword","showConfirmPassword","showPasswordError","form","nome","cognome","gender","address","cap_code","tax_code","telefono","email","password","confirmPassword","methods","goToNextStep","isStepValid","goToPreviousStep","step","passwordsMatch","toggleShowPassword","toggleShowConfirmPassword","onSubmit","console","log","setTimeout"],"sources":["/Users/alessandromagnani/Desktop/Università/applicazioni-e-servizi-web/osteoarthritis-project/frontend/src/components/UserRegister.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <div class=\"container mt-5\">\n      <div class=\"header\">\n        <button\n          v-if=\"currentStep > 1\"\n          class=\"back-button\"\n          @click=\"goToPreviousStep\"\n        >\n          <img src=\"@/assets/back-icon.svg\" alt=\"Indietro\" class=\"back-icon\" />\n        </button>\n      </div>\n\n      <span class=\"step-title\">Passaggio {{ currentStep }} di 3</span>\n      <h2 class=\"mb-4\">Crea un Account</h2>\n\n      <form @submit.prevent=\"onSubmit\">\n        <div v-if=\"currentStep === 1\">\n          <div class=\"mb-3\">\n            <label for=\"nome\" class=\"form-label small-label\">Nome</label>\n            <input\n              id=\"nome\"\n              v-model=\"form.nome\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"cognome\" class=\"form-label small-label\">Cognome</label>\n            <input\n              id=\"cognome\"\n              v-model=\"form.cognome\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"gender\" class=\"form-label small-label\">Genere</label>\n            <select\n              id=\"gender\"\n              v-model=\"form.gender\"\n              class=\"form-control\"\n              required\n            >\n              <option value=\"Male\">Maschio</option>\n              <option value=\"Female\">Femmina</option>\n            </select>\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"data\" class=\"form-label small-label\"\n              >Data di Nascita</label\n            >\n            <input\n              id=\"data\"\n              v-model=\"form.data\"\n              type=\"date\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n        </div>\n\n        <div v-if=\"currentStep === 2\">\n          <div class=\"mb-3\">\n            <label for=\"address\" class=\"form-label small-label\"\n              >Indirizzo</label\n            >\n            <input\n              id=\"address\"\n              v-model=\"form.address\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"cap_code\" class=\"form-label small-label\">CAP</label>\n            <input\n              id=\"cap_code\"\n              v-model=\"form.cap_code\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"tax_code\" class=\"form-label small-label\"\n              >Codice Fiscale</label\n            >\n            <input\n              id=\"tax_code\"\n              v-model=\"form.tax_code\"\n              type=\"text\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"telefono\" class=\"form-label small-label\"\n              >Numero di Telefono</label\n            >\n            <input\n              id=\"telefono\"\n              v-model=\"form.telefono\"\n              type=\"tel\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n        </div>\n\n        <div v-if=\"currentStep === 3\">\n          <div class=\"mb-3\">\n            <label for=\"email\" class=\"form-label small-label\">Email</label>\n            <input\n              id=\"email\"\n              v-model=\"form.email\"\n              type=\"email\"\n              class=\"form-control\"\n              required\n            />\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"password\" class=\"form-label small-label\"\n              >Password\n              <span\n                class=\"info-icon\"\n                title=\"Requisiti per la password: min 8 caratteri, almeno una maiuscola, una minuscola e un numero.\"\n              >\n                <img\n                  src=\"@/assets/info-icon.svg\"\n                  alt=\"Info\"\n                  class=\"info-image\"\n                />\n              </span>\n            </label>\n            <div class=\"input-group\">\n              <input\n                id=\"password\"\n                v-model=\"form.password\"\n                :type=\"showPassword ? 'text' : 'password'\"\n                class=\"form-control password-input\"\n                required\n                @paste.prevent\n              />\n              <button\n                type=\"button\"\n                class=\"btn eye-button\"\n                @click=\"toggleShowPassword\"\n              >\n                <img\n                  :src=\"\n                    showPassword\n                      ? require('@/assets/eye-off.svg')\n                      : require('@/assets/eye-on.svg')\n                  \"\n                  alt=\"Toggle Password Visibility\"\n                  class=\"eye-icon\"\n                />\n              </button>\n            </div>\n            <div v-if=\"showPasswordError\" class=\"text-danger mt-1\">\n              Le password non corrispondono.\n            </div>\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"confirmPassword\" class=\"form-label small-label\"\n              >Conferma Password</label\n            >\n            <div class=\"input-group\">\n              <input\n                id=\"confirmPassword\"\n                v-model=\"form.confirmPassword\"\n                :type=\"showConfirmPassword ? 'text' : 'password'\"\n                class=\"form-control password-input\"\n                :class=\"{ 'is-invalid': showPasswordError }\"\n                required\n                @paste.prevent\n              />\n              <button\n                type=\"button\"\n                class=\"btn eye-button\"\n                @click=\"toggleShowConfirmPassword\"\n              >\n                <img\n                  :src=\"\n                    showConfirmPassword\n                      ? require('@/assets/eye-off.svg')\n                      : require('@/assets/eye-on.svg')\n                  \"\n                  alt=\"Toggle Password Visibility\"\n                  class=\"eye-icon\"\n                />\n              </button>\n            </div>\n          </div>\n        </div>\n\n        <button\n          type=\"button\"\n          class=\"btn btn-primary btn-next\"\n          :disabled=\"!isStepValid(currentStep)\"\n          @click=\"goToNextStep\"\n        >\n          Avanti\n        </button>\n      </form>\n    </div>\n\n    <div v-if=\"loading\" class=\"loading-overlay\">\n      <div class=\"loading-icon\">\n        <img src=\"@/assets/loading-icon.svg\" alt=\"Loading...\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      currentStep: 1,\n      loading: false,\n      showPassword: false,\n      showConfirmPassword: false,\n      showPasswordError: false,\n      form: {\n        nome: \"\",\n        cognome: \"\",\n        gender: \"\",\n        data: \"\",\n        address: \"\",\n        cap_code: \"\",\n        tax_code: \"\",\n        telefono: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      },\n    };\n  },\n  methods: {\n    goToNextStep() {\n      if (this.isStepValid(this.currentStep)) {\n        this.currentStep++;\n      }\n    },\n    goToPreviousStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    isStepValid(step) {\n      switch (step) {\n        case 1: {\n          return (\n            this.form.nome &&\n            this.form.cognome &&\n            this.form.gender &&\n            this.form.data\n          );\n        }\n        case 2: {\n          return (\n            this.form.address &&\n            this.form.cap_code &&\n            this.form.tax_code &&\n            this.form.telefono\n          );\n        }\n        case 3: {\n          const passwordsMatch =\n            this.form.password === this.form.confirmPassword;\n          this.showPasswordError = !passwordsMatch;\n          return this.form.email && this.form.password && passwordsMatch;\n        }\n        default:\n          return false;\n      }\n    },\n    toggleShowPassword() {\n      this.showPassword = !this.showPassword;\n    },\n    toggleShowConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n    },\n    onSubmit() {\n      this.loading = true; // Mostra il caricamento\n      console.log(this.form);\n      // Simula un caricamento\n      setTimeout(() => {\n        this.loading = false; // Nascondi il caricamento dopo 2 secondi\n      }, 2000);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  background: #ffffff; /* Sfondo bianco */\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.container {\n  max-width: 600px;\n  padding: 40px;\n  border-radius: 15px;\n  background: #ffffff; /* Sfondo del contenitore bianco */\n  box-shadow: 0 12px 24px rgba(0, 0, 0, 0.1);\n  margin-top: 100px;\n}\n\n.form-label {\n  font-size: 0.9em; /* Dimensione più piccola per le etichette */\n}\n\n.eye-button {\n  width: 40px; /* Larghezza maggiore per i pulsanti eye */\n}\n\n.btn-next {\n  height: 40px; /* Altezza ridotta per il pulsante Avanti */\n  width: 100%; /* Larghezza del pulsante Avanti */\n}\n\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.loading-icon img {\n  width: 50px; /* Dimensioni dell'icona di caricamento */\n}\n</style>\n"],"mappings":"AAoOA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,OAAO,EAAE,KAAK;MACdC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,iBAAiB,EAAE,KAAK;MACxBC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVT,IAAI,EAAE,EAAE;QACRU,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE;MACnB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACC,WAAW,CAAC,IAAI,CAAClB,WAAW,CAAC,EAAE;QACtC,IAAI,CAACA,WAAW,EAAE;MACpB;IACF,CAAC;IACDmB,gBAAgBA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACnB,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF,CAAC;IACDkB,WAAWA,CAACE,IAAI,EAAE;MAChB,QAAQA,IAAI;QACV,KAAK,CAAC;UAAE;YACN,OACE,IAAI,CAACf,IAAI,CAACC,IAAG,IACb,IAAI,CAACD,IAAI,CAACE,OAAM,IAChB,IAAI,CAACF,IAAI,CAACG,MAAK,IACf,IAAI,CAACH,IAAI,CAACN,IAAG;UAEjB;QACA,KAAK,CAAC;UAAE;YACN,OACE,IAAI,CAACM,IAAI,CAACI,OAAM,IAChB,IAAI,CAACJ,IAAI,CAACK,QAAO,IACjB,IAAI,CAACL,IAAI,CAACM,QAAO,IACjB,IAAI,CAACN,IAAI,CAACO,QAAO;UAErB;QACA,KAAK,CAAC;UAAE;YACN,MAAMS,cAAa,GACjB,IAAI,CAAChB,IAAI,CAACS,QAAO,KAAM,IAAI,CAACT,IAAI,CAACU,eAAe;YAClD,IAAI,CAACX,iBAAgB,GAAI,CAACiB,cAAc;YACxC,OAAO,IAAI,CAAChB,IAAI,CAACQ,KAAI,IAAK,IAAI,CAACR,IAAI,CAACS,QAAO,IAAKO,cAAc;UAChE;QACA;UACE,OAAO,KAAK;MAChB;IACF,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACpB,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IACDqB,yBAAyBA,CAAA,EAAG;MAC1B,IAAI,CAACpB,mBAAkB,GAAI,CAAC,IAAI,CAACA,mBAAmB;IACtD,CAAC;IACDqB,QAAQA,CAAA,EAAG;MAC